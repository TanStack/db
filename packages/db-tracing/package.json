{
  "name": "@tanstack/db-tracing",
  "description": "Tracing utilities for TanStack DB",
  "version": "0.0.0",
  "dependencies": {},
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.0.9"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "packageManager": "pnpm@10.6.3",
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "typescript": ">=4.7"
  },
  "author": "Sam Willis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/db.git",
    "directory": "packages/db-tracing"
  },
  "homepage": "https://tanstack.com/db",
  "keywords": [
    "tanstack",
    "db",
    "tracing",
    "opentelemetry",
    "performance"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --fix",
    "test": "npx vitest --run"
  },
  "sideEffects": false,
  "type": "module",
  "types": "dist/esm/index.d.ts"
}
