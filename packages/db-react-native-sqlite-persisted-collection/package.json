{
  "name": "@tanstack/db-react-native-sqlite-persisted-collection",
  "version": "0.1.0",
  "description": "React Native and Expo SQLite persisted collection adapter for TanStack DB",
  "author": "TanStack Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/db.git",
    "directory": "packages/db-react-native-sqlite-persisted-collection"
  },
  "homepage": "https://tanstack.com/db",
  "keywords": [
    "sqlite",
    "react-native",
    "expo",
    "persistence",
    "typescript"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --fix",
    "test": "vitest --run",
    "test:e2e": "pnpm --filter @tanstack/db-ivm build && pnpm --filter @tanstack/db build && pnpm --filter @tanstack/db-sqlite-persisted-collection-core build && pnpm --filter @tanstack/db-react-native-sqlite-persisted-collection build && vitest --config vitest.e2e.config.ts --run",
    "test:e2e:runtime": "pnpm --filter @tanstack/db-ivm build && pnpm --filter @tanstack/db build && pnpm --filter @tanstack/db-sqlite-persisted-collection-core build && pnpm --filter @tanstack/db-react-native-sqlite-persisted-collection build && TANSTACK_DB_MOBILE_REQUIRE_RUNTIME_FACTORY=1 vitest --config vitest.e2e.config.ts --run"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./react-native": {
      "import": {
        "types": "./dist/esm/react-native.d.ts",
        "default": "./dist/esm/react-native.js"
      },
      "require": {
        "types": "./dist/cjs/react-native.d.cts",
        "default": "./dist/cjs/react-native.cjs"
      }
    },
    "./expo": {
      "import": {
        "types": "./dist/esm/expo.d.ts",
        "default": "./dist/esm/expo.js"
      },
      "require": {
        "types": "./dist/cjs/expo.d.cts",
        "default": "./dist/cjs/expo.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@tanstack/db-sqlite-persisted-collection-core": "workspace:*"
  },
  "peerDependencies": {
    "@op-engineering/op-sqlite": "^15.2.5",
    "typescript": ">=4.7"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@vitest/coverage-istanbul": "^3.2.4",
    "better-sqlite3": "^12.6.2"
  }
}
