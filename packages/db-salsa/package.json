{
  "name": "@tanstack/db-salsa",
  "version": "0.0.1",
  "description": "Salsa-style incremental computation for TanStack DB - fine-grained dependency tracking and memoization",
  "author": "TanStack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/db.git",
    "directory": "packages/db-salsa"
  },
  "homepage": "https://tanstack.com/db",
  "keywords": [
    "salsa",
    "incremental",
    "computation",
    "memoization",
    "dependency-tracking",
    "typescript"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --fix",
    "test": "vitest --run"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "typescript": ">=4.7"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.2.4"
  }
}
