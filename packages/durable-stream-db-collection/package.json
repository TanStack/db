{
  "name": "@tanstack/durable-stream-db-collection",
  "description": "Durable Streams collection for TanStack DB",
  "version": "0.0.1",
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "@tanstack/db": "workspace:*"
  },
  "peerDependencies": {
    "@durable-streams/client": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@durable-streams/client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.2.4",
    "zod": "^3.23.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "packageManager": "pnpm@10.24.0",
  "author": "TanStack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/db.git",
    "directory": "packages/durable-stream-db-collection"
  },
  "homepage": "https://tanstack.com/db",
  "keywords": [
    "durable-streams",
    "streaming",
    "sync",
    "optimistic",
    "typescript"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --fix",
    "test": "npx vitest run"
  },
  "sideEffects": false,
  "type": "module",
  "types": "dist/esm/index.d.ts"
}
